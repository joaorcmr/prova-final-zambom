{
  "name": "prova-final-zambom",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "prestart": "npm run migration:run",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "prestart:dev": "cross-env NODE_ENV=development npm run migration:run",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "prestart:debug": "cross-env NODE_ENV=development npm run migration:run",
    "start:test": "cross-env NODE_ENV=test nest start --watch",
    "prestart:test": "cross-env NODE_ENV=test npm run migration:run",
    "start:staging": "cross-env NODE_ENV=staging nest start --watch",
    "prestart:staging": "cross-env NODE_ENV=staging npm run migration:run",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "prestart:prod": "cross-env NODE_ENV=production npm run migration:run",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:integration": "cross-env NODE_ENV=test jest --config ./jest.integration.config.js --runInBand",
    "test:integration:watch": "cross-env NODE_ENV=test jest --config ./jest.integration.config.js --watch --runInBand",
    "test:all": "npm run test && npm run test:integration",
    "test:ci": "npm run test:cov && npm run test:integration",
    "migration:generate": "typeorm migration:create",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d ./src/infra/db/data-source.ts",
    "migration:run:dev": "cross-env NODE_ENV=development typeorm-ts-node-commonjs migration:run -d ./src/infra/db/data-source.ts",
    "migration:run:test": "cross-env NODE_ENV=test typeorm-ts-node-commonjs migration:run -d ./src/infra/db/data-source.ts",
    "migration:run:staging": "cross-env NODE_ENV=staging typeorm-ts-node-commonjs migration:run -d ./src/infra/db/data-source.ts",
    "migration:run:prod": "cross-env NODE_ENV=production typeorm-ts-node-commonjs migration:run -d ./src/infra/db/data-source.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "cross-env": "^10.1.0",
    "firebase": "^12.5.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
